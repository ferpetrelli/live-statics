{
    "name": "petrelli/live-statics",
    "description": "Rapid development aid package that facilitates data modelling and dynamic integration with views",
    "type": "library",
    "license": "MIT",
    "keywords": [
      "statics",
      "injection",
      "data",
      "controller",
      "services",
      "mock"
    ],
    "authors": [
        {
            "name": "Fernando Petrelli",
            "email": "contact@fernandopetrelli.com"
        }
    ],
    "require": {
      "php": "^7.0",
      "laravel/framework": "~5.6.0|~5.7.0"
    },
    "require-dev": {
      "mockery/mockery": "^1.0",
      "orchestra/testbench": "~3.0",
      "phpunit/phpunit": "^7.5"
    },
    "autoload": {
      "psr-4": {
        "Petrelli\\LiveStatics\\": "src/",
        "Petrelli\\LiveStatics\\Tests\\Extras\\": "tests/Extras/"
      }
    },
    "extra": {
      "laravel": {
          "providers": [
              "Petrelli\\LiveStatics\\BaseServiceprovider"
          ]
      }
    },
    "minimum-stability": "stable"
}
